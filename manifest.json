{
  "manifest_version": 3,
  "name": "mydnr",
  "version": "0",
  "description": "My custom network filters in Brave browser",
  "icons": {
    "128": "on.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "management"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "0",
        "path": "0.json",
        "enabled": true
      }
    ]
  },
  "background": {
    "service_worker": "bg.js"
  },
  "action": {}
}